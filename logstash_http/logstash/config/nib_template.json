{
  "order": 1000,
  "index_patterns": ["nib*"],
  "version": 50043,
  "settings": {
    "index": {
      "number_of_shards": "1",
      "codec": "best_compression",
      "number_of_replicas": "1"
    }
  },
  "mappings": {
    "doc": {
      "dynamic": true,
      "numeric_detection": true,
      "dynamic_templates": [
        {
          "message_field": {
            "match_mapping_type": "string",
            "mapping": {
              "type": "keyword"
            }
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "user_id": {
          "type": "keyword"
        }
      }
    }
  },
  "aliases": {}
}

